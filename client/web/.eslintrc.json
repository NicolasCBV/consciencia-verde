{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "standard", "prettier", "eslint:recommended", "next/core-web-vitals"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint", "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-useless-constructor": "off"
  }
}
